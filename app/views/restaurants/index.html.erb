<h1>Restaurants in the area</h1>

<p><i class="fas fa-plus"></i> <%= link_to "Add restaurant", new_restaurant_path() %></p>

<% @restaurants.each do |restaurant| %>
  <h3><%= restaurant.id %>) <%= link_to restaurant.name, restaurant_path(restaurant) %></h3>
  <p><%= restaurant.category.capitalize %></p>
<% end %>
